            <section class="row" id="reports">
              <h1 class="page-header">Monitors</h1>
              <table class="table table-striped table-hover">
                
                <caption>Service Errors</caption>
                
                <thead>
                  <tr><th>ID</th><th>Service Name</th><th>Last Logged At</th><th>Status</th><th>Reset</th></tr>
                </thead>
                
                <tfoot>
                  <tr>
                    <td colspan="5">Showing {{errorCount}} monitors.</td>
                  </tr>
                </tfoot>
                
                <tbody>
{{view Canary.ErrorMonitorTableRowView}}
                </tbody>

              </table>

              <table class="table table-striped table-hover">
                
                <caption>Service Warnings</caption>
                
                <thead>
                  <tr><th>ID</th><th>Service Name</th><th>Last Logged At</th><th>Status</th><th>Average</th><th>Response</th></tr>
                </thead>
                
                <tfoot>
                  <tr>
                    <td colspan="6">Showing {{warnCount}} monitors.</td>
                  </tr>
                </tfoot>
                
                <tbody>
{{view Canary.WarningMonitorTableRowView}}
                </tbody>

              </table>
                            
              <table class="table table-striped table-hover">
                
                <caption>Service Errors</caption>
                
                <thead>
                  <tr><th>ID</th><th>Service Name</th><th>Last Logged At</th><th>Status</th><th>Average</th><th>Response</th></tr>
                </thead>
                
                <tfoot>
                  <tr>
                    <td colspan="6">Showing {{normalCount}} monitors.</td>
                  </tr>
                </tfoot>
                
                <tbody>
{{view Canary.NormalMonitorTableRowView}}
                </tbody>

              </table>

            </section>